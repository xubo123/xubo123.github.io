<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hxy.core.clientrelease.dao.ClientMapper">
	<resultMap type="com.hxy.core.clientrelease.entity.Client" id="clientResultMap">
		<id column="id" property="id"/>
		<result column="version" property="version"/>
		<result column="url" property="url"/>
		<result column="create_time" property="createTime"/>
	</resultMap>
	<select id="countClient" parameterType="com.hxy.core.clientrelease.entity.ClientModel" useCache="true" flushCache="false" resultType="long">
		select count(id) from apk_version
	</select>
	<select id="selectClient" parameterType="com.hxy.core.clientrelease.entity.ClientModel" useCache="true" flushCache="false" resultMap="clientResultMap">
		select * from apk_version limit #{start},#{end}
	</select>
	<insert id="addClient" parameterType="com.hxy.core.clientrelease.entity.ClientModel" flushCache="true">
		insert into apk_version(version,url,create_time,type) values(#{version},#{url},#{createTime},#{type});
	</insert>
	<update id="updateClient" parameterType="com.hxy.core.clientrelease.entity.ClientModel" flushCache="true">
		update apk_version
		<set>
			<if test="version!=null and version!=''">
				version=#{version},
			</if>
			<if test="url!=null and url!=''">
				url=#{url}
			</if>
		</set>
		where id=#{id}
	</update>
	<select id="selectById" parameterType="int" useCache="false" resultMap="clientResultMap">
		select * from apk_version where id=#{id}
	</select>
	<select id="selectNewOne" resultMap="clientResultMap">
		select * from apk_version order by id desc limit 0,1
	</select>
</mapper>